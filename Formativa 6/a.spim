	.data
str: 	.asciiz "\n"
	.text

main:
	li $v0, 5		
	syscall			# Le um inteiro
	
	move $t0, $v0		# Armazena no registrador
	
	li $v0, 5		
	syscall			# Le um inteiro
	
	move $t1, $v0		# Armazena no registrador
	
	sub $t2, $t0, $t1	# Faz a ubtração
	 
	li $v0, 1		
	move $a0, $t2		# Movendo o conteúdo do registrador para o argumento
	syscall			# Imprime o resultado da subtração
	
	li $v0, 4		# Imprime uma end-line
	la $a0, str
	syscall
	
	li $v0, 10		# Encerra o programa
	syscall